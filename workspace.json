{
  "version": 1,
  "projects": {
    "st-ui": {
      "root": "libs/st-ui",
      "sourceRoot": "libs/st-ui/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "libs/st-ui/.eslintrc",
            "tsConfig": [
              "libs/st-ui/tsconfig.lib.json",
              "libs/st-ui/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/st-ui/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/st-ui/jest.config.js",
            "tsConfig": "libs/st-ui/tsconfig.spec.json"
          }
        }
      }
    },
    "ng-ui": {
      "projectType": "library",
      "root": "libs/ng-ui",
      "sourceRoot": "libs/ng-ui/src",
      "prefix": "gds",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/ng-ui/tsconfig.lib.json",
              "libs/ng-ui/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/ng-ui/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/ng-ui/jest.config.js",
            "tsConfig": "libs/ng-ui/tsconfig.spec.json",
            "setupFile": "libs/ng-ui/src/test-setup.ts"
          }
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "styleext": "scss"
        }
      }
    },
    "ra-ui": {
      "root": "libs/ra-ui",
      "sourceRoot": "libs/ra-ui/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "libs/ra-ui/.eslintrc",
            "tsConfig": [
              "libs/ra-ui/tsconfig.lib.json",
              "libs/ra-ui/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/ra-ui/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/ra-ui/jest.config.js",
            "tsConfig": "libs/ra-ui/tsconfig.spec.json"
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/workspace"
  },
  "schematics": {
    "@nrwl/workspace": {
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "eslint"
      }
    },
    "@nrwl/react": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/next": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/web": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/nx-plugin": {
      "plugin": {
        "linter": "eslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "eslint"
      }
    }
  }
}
